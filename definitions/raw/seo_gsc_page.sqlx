config {
  name: "seo_gsc_page",
  schema: "seo_dashboard_02_raw",
  type: "incremental",
  uniqueKey: ["day", "site", "page"]
}

with up_to_date as (
  select
    * except (dt_created, dt_updated, dt_filename),
    row_number() over (
      partition by day, site, page
      order by dt_updated desc
    ) rank
  from ${ref("seo_dashboard_01_temp", "temp_seo_gsc_page")}
)
select 
  * except (rank)
from up_to_date
where rank = 1

